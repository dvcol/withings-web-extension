{
  "name": "@dvcol/withings-web-extension",
  "type": "module",
  "version": "1.0.0",
  "title": "Web Extension Template",
  "packageManager": "pnpm@9.1.0",
  "description": "Web Extension Template",
  "license": "MIT",
  "author": "dvcol",
  "homepage": "https://github.com/dvcol/web-extension-template",
  "bugs": "https://github.com/dvcol/web-extension-template/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvcol/web-extension-template.git"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">= 7"
  },
  "files": [
    "*.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "husky",
    "clear": "rimraf dist",
    "dist": "pnpm run clear && esno scripts/prepare.ts",
    "type:check": "tsc --noEmit",
    "vite:serve": "vite",
    "vite:build": "vite build",
    "vite:preview": "vite preview",
    "dev": "cross-env NODE_ENV=development run-p dist type:check vite:serve",
    "build": "cross-env NODE_ENV=production run-s dist 'vite:build {@}' --",
    "analyse": "vite-bundle-visualizer",
    "test:unit": "vitest run --coverage --mode testing",
    "test:watch": "vitest --mode testing",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "style": "stylelint **/*.{svelete,css,scss,less,html} --go '{\"gitignore\":true}'",
    "style:fix": "pnpm run style --fix",
    "release": "standard-version",
    "release:changelog": "extract-changelog-release > RELEASE.md"
  },
  "dependencies": {
    "@dvcol/web-extension-utils": "^2.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@dvcol/eslint-plugin-presets": "^1.3.10",
    "@dvcol/stylelint-plugin-presets": "^2.1.2",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@tsconfig/node22": "^22.0.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/chrome": "^0.0.268",
    "@types/fs-extra": "^11.0.1",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^20.12.12",
    "@types/webextension-polyfill": "^0.10.7",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.1.0",
    "c8": "^9.0.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^8.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-svelte": "2.32.2",
    "eslint-plugin-vitest": "^0.4.0",
    "eslint-plugin-yml": "^1.0.0",
    "esno": "^4.7.0",
    "extract-changelog-release": "^1.0.2",
    "fs-extra": "^11.1.1",
    "husky": "^9.0.6",
    "jsdom": "^24.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^15.0.0",
    "magic-string": "^0.30.3",
    "npm-run-all2": "^6.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^2.10.1",
    "rimraf": "^5.0.1",
    "rollup": "^4.18.0",
    "sass": "^1.77.2",
    "standard-version": "^9.5.0",
    "stylelint": "^16.2.0",
    "svelte": "^4.2.17",
    "svelte-check": "^3.7.1",
    "svelte-preprocess": "^5.1.4",
    "svelte-spa-router": "^4.0.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-bundle-visualizer": "^1.0.0",
    "vite-plugin-checker": "^0.6.2",
    "vitest": "^1.1.0",
    "webextension-polyfill": "^0.10.0",
    "yaml-eslint-parser": "^1.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,svelte,json,md}": [
      "eslint --fix"
    ],
    "*.{css,scss,less,vue,svelte}": [
      "stylelint --fix"
    ]
  }
}
